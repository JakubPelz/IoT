end/src/components/productsComponents/ProductsComponent.js",["141","142","143"],[],"import React from 'react';\n\nconst ProductsComponent = (props) => {\n  const { products, onAdd } = props;\n\n  return (\n    <div className=\"ui grid center aligner segmant\">\n      {products.map((product) => {\n        return (\n          <div className=\"five wide column\" key={product.image}>\n            <div className=\"ui card\" key={product.id}>\n              <div className=\"image\">\n                <img src={product.image} />\n              </div>\n              <div className=\"content\">\n                <a className=\"header\">{product.name}</a>\n                <div className=\"meta\" style={{ marginTop: '5px' }}>\n                  <span className=\"date\">Cena: {product.price}Kč</span>\n                </div>\n                <div className=\"description\">\n                  <h4>Ingredience</h4>\n                </div>\n                <div className=\"description\" style={{ marginTop: '10px' }}>\n                  {product.ingredients.map((ingredient, i) => {\n                    return <li key={i}> {ingredient} </li>;\n                  })}\n                </div>\n              </div>\n              <div className=\"extra content\">\n                <a onClick={() => onAdd(product)}>\n                  <i className=\"shop icon\"></i>\n                  Přidat do košíku\n                </a>\n              </div>\n            </div>\n          </div>\n        );\n      })}\n    </div>\n  );\n};\n\nexport default ProductsComponent;\n","/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/productsComponents/Data.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/productsComponents/Baskets.js",["144","145"],[],"import React, { useState } from 'react';\nimport { CardContext } from '../context/CardContext';\nimport { Navigate } from 'react-router';\n\nconst Baskets = (props) => {\n  const { cartItems, onAdd, onRemove } = props;\n  const { setShopCard } = React.useContext(CardContext);\n\n  const [redirect, setRedirect] = useState(false);\n\n  const itemsPrice = cartItems.reduce((a, c) => a + c.price * c.qty, 0);\n  const taxPrice = itemsPrice * 0.15;\n  const finalPrice = itemsPrice + taxPrice;\n\n  const submit = () => {\n    const response = { cartItems, itemsPrice, taxPrice, finalPrice };\n    setShopCard(response);\n    setRedirect(true);\n  };\n\n  if (redirect) {\n    return <Navigate to={'/final'} />;\n  }\n\n  return (\n    <div>\n      {' '}\n      <h2>Cart Items</h2>\n      <div>{cartItems.length === 0 && <div>Náupní košík je prázdný.</div>}</div>\n      <div className=\"ui grid center aligner segmant\">\n        {cartItems.map((item) => (\n          <div key={item.id} className=\"five wide column\">\n            <strong>{item.name}</strong> cena bez DPH: {item.price.toFixed(0)}{' '}\n            Kč/1ks\n            <div className=\"ui card\">\n              <div className=\"extra content\">\n                <span className=\"left floated like\">\n                  <a onClick={() => onRemove(item)}>\n                    <i className=\"minus icon\"></i>\n                  </a>\n                </span>\n                <span style={{ paddingLeft: '10px', color: 'black' }}>\n                  V košíku máte: {item.qty} Ks\n                </span>\n                <span className=\"right floated star\">\n                  <a onClick={() => onAdd(item)}>\n                    <i className=\"plus icon\"></i>\n                  </a>\n                </span>\n              </div>\n            </div>\n          </div>\n        ))}\n      </div>\n      <div className=\"ui right aligned container\" style={{ marginTop: '20px' }}>\n        <div style={{ fontSize: '20px', fontWeight: 'bold' }}>Cena celkem:</div>\n        <div className=\"ui list\">\n          <div className=\"item\">\n            Cena celkem bez DPH: {itemsPrice.toFixed(0)} Kč\n          </div>\n          <div className=\"item\">DPH: {taxPrice.toFixed(0)} Kč</div>\n          <div className=\"item\">\n            Cena Celkem s DPH: {finalPrice.toFixed(0)} Kč\n          </div>\n        </div>\n        <button\n          className=\"ui blue button\"\n          type=\"submit\"\n          style={{ marginTop: '30px', width: '200px' }}\n          onClick={submit}\n        >\n          Potvrdit\n        </button>\n      </div>\n    </div>\n  );\n};\n\nexport default Baskets;\n","/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/pages/FinalPage.jsx",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/navBar/TopNavBar.jsx",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/finalPageComponents/ProductsFinal.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/finalPageComponents/UserFinal.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/finalPageComponents/FinalPrice.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/finalPageComponents/ForeignCurrency.js",["146"],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/finalPageComponents/EurProductsFinal.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/finalPageComponents/EurFinalPrice.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/context/ForeignContext.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/inputs/ZipInput.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/inputs/AddressInput.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/inputs/CityInput.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/inputs/EmailInput.js",[],[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/inputs/CompanyInput.js",[],[],"/Users/JakubMac/Documents/GitHub/Fapi/frontend/src/components/inputs/NameInput.js",[],[],{"ruleId":"147","severity":1,"message":"148","line":13,"column":17,"nodeType":"149","endLine":13,